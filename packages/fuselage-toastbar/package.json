{
  "name": "@rocket.chat/fuselage-toastbar",
  "version": "0.33.0",
  "description": "Fuselage ToastBar component",
  "keywords": [
    "rocketchat",
    "rocketchat-fuselage",
    "fuselage",
    "toastbar",
    "fuselage-toastbar"
  ],
  "author": {
    "name": "Rocket.Chat",
    "url": "https://rocket.chat/"
  },
  "homepage": "https://github.com/RocketChat/fuselage#readme",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RocketChat/fuselage.git",
    "directory": "packages/fuselage-toastbar"
  },
  "bugs": {
    "url": "https://github.com/RocketChat/fuselage/issues"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "run-s .:build:clean .:build:esm .:build:cjs",
    ".:build:clean": "rimraf dist",
    ".:build:esm": "tsc -p tsconfig-esm.json",
    ".:build:cjs": "tsc -p tsconfig-cjs.json",
    "lint": "lint",
    "lint-and-fix": "lint-and-fix",
    "test": "jest --runInBand",
    "docs": "typedoc",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "bump-next": "bump-next"
  },
  "devDependencies": {
    "@rocket.chat/eslint-config-alt": "^0.31.25",
    "@rocket.chat/fuselage": "^0.57.1",
    "@rocket.chat/fuselage-hooks": "^0.33.1",
    "@rocket.chat/layout": "^0.31.27",
    "@rocket.chat/prettier-config": "^0.31.25",
    "@rocket.chat/styled": "^0.31.25",
    "@storybook/addon-essentials": "~6.5.16",
    "@storybook/addons": "~6.5.16",
    "@storybook/builder-webpack5": "~6.5.16",
    "@storybook/manager-webpack5": "~6.5.16",
    "@storybook/react": "~6.5.16",
    "@storybook/theming": "~6.5.16",
    "@types/jest": "~29.5.0",
    "@types/react": "~17.0.80",
    "@types/react-dom": "~17.0.25",
    "bump": "^0.31.25",
    "eslint": "~8.38.0",
    "jest": "~29.5.0",
    "lint-all": "^0.31.25",
    "npm-run-all": "^4.1.5",
    "prettier": "~2.8.7",
    "rimraf": "~3.0.2",
    "storybook-dark-mode": "~1.1.2",
    "ts-jest": "~29.1.0",
    "typedoc": "~0.25.13",
    "typescript": "~5.5.4"
  },
  "peerDependencies": {
    "@rocket.chat/fuselage": "*",
    "@rocket.chat/fuselage-hooks": "*",
    "@rocket.chat/fuselage-polyfills": "*",
    "@rocket.chat/styled": "*",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "eslintConfig": {
    "extends": "@rocket.chat/eslint-config-alt/typescript",
    "env": {
      "jest": true
    }
  },
  "prettier": "@rocket.chat/prettier-config/fuselage",
  "jest": {
    "preset": "ts-jest",
    "errorOnDeprecated": true,
    "testMatch": [
      "<rootDir>/src/**/*.spec.[jt]s?(x)"
    ],
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect",
      "testing-utils/setup/noErrorsLogged"
    ]
  },
  "volta": {
    "extends": "../../package.json"
  },
  "dependencies": {
    "react-timing-hooks": "~4.0.2"
  }
}
